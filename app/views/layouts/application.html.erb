<%= render 'layouts/header' %>
<body>
  <section class="round">
    <% flash.each do |key, value| %>
      <div class="flash <%= key %>"><%= value %></div>
    <% end %>
    <%= yield %>
  </section>
  <div id="debug_info">
    <%= debug(params) if Rails.env.development? %>
  </div>
</body>
</html>
